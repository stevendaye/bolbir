    "lodash": "^4.17.15",
    "npm-run-all": "^4.1.5",
    "open": "^6.4.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "serve-favicon": "^2.5.0",
    "style-loader": "^1.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0"
  }
